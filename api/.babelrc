{
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry", // alternative mode: "entry"
          "corejs": 3, // default would be 2
          "targets": {"esmodules": true}
          // set your own target environment here (see Browserslist)
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
}